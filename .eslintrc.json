{
  "extends": "eslint-config-appium",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    // Add any custom rules here
    "promise/no-native": "off",
    // TODO: Bring this back once use winston
    "no-console": "off"
  },
  "ignorePatterns": ["*.pipeline.js"]
}
